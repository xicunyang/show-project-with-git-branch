{
  "name": "show-project-with-git-branch",
  "displayName": "Find Project By Branch Name",
  "publisher": "xicunyang",
  "description": "你同时管理N个项目？N个项目又有M个复制工程？每个工程里有自己的分支？找不到分支对应的项目了？来试试这个！",
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "show-project-with-git-branch-panel",
          "title": "Projects",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "show-project-with-git-branch-panel": [
        {
          "id": "show-project-with-git-branch-panel-view",
          "type": "webview",
          "name": "show-project-with-git-branch-panel",
          "container": "show-project-with-git-branch-panel"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.openLeftPanel",
        "key": "ctrl+r",
        "mac": "ctrl+r"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.4"
  }
}
